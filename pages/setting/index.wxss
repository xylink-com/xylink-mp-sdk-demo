.setting-container {
  width: 100%;
  padding: 0PX 20px 20PX;
  box-sizing: border-box;
  font-size: 14PX;
}
.setting-title{
  font-size: 16PX;
  margin-top: 30PX;
}
.form-input {
  border: 1px solid #ddd;
  font-size: 15PX;
  padding: 0 10rpx;
  height: 80rpx;
  margin: 10px 0 30px;
}

.form-audio{
  display: inline-flex;
  align-items: center;
  margin-top: 10PX;
  margin-right: 20PX;
}

.from-audio-group{
  display: flex;
}

.btn {
  margin-bottom: 10PX;
  font-size: 14PX;
}
.form-radio .wx-radio-input.wx-radio-input-checked{
  border-color:#3876FF !important;
  background:#3876FF !important;
}
.form-radio .wx-radio-input.wx-radio-input-checked::before{
  background:#3876FF;
}

.btn-primary{
  background-color: #3876FF;
  color: #fff;
}

.version{
  width: 100%;
  font-size: 12PX;
  color: rgba(0, 0, 0, 0.6);
  margin: 10PX 0;
}